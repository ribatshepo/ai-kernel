{
  "Caching": {
    "Redis": {
      "ConnectionString": "localhost:6379,password=your_password_here",
      "InstanceName": "aikernel_",
      "ConnectTimeout": 5000,
      "SyncTimeout": 5000,
      "ConnectRetry": 3,
      "AbortOnConnectFail": false,
      "UseSsl": false,
      "SslHost": null,
      "SentinelConfiguration": null
    },
    "Defaults": {
      "ResourceCacheTtlSeconds": 300,
      "SearchResultsTtlSeconds": 600,
      "VersionHistoryTtlSeconds": 3600,
      "LineageQueryTtlSeconds": 900,
      "RelationshipCacheTtlSeconds": 600,
      "EnableCompression": true,
      "CompressionThresholdBytes": 1024
    },
    "Metrics": {
      "Enabled": true,
      "PublishIntervalSeconds": 60,
      "EnableDetailedMetrics": true
    }
  }
}
